{
  "version": "1.0",
  "name": "Comprehensive Module Test - 100+ Nodes",
  "description": "Extensive workflow testing as many modules as possible with deterministic data generation, multiple transformations, and table output",
  "trigger": {
    "type": "manual",
    "config": {}
  },
  "config": {
    "steps": [
      {
        "id": "step1",
        "module": "utilities.array-utils.range",
        "inputs": {
          "start": 1,
          "end": 20,
          "step": 1
        },
        "outputAs": "baseNumbers"
      },
      {
        "id": "step2",
        "module": "utilities.array-utils.shuffle",
        "inputs": {
          "array": "{{baseNumbers}}"
        },
        "outputAs": "shuffledNumbers"
      },
      {
        "id": "step3",
        "module": "utilities.array-utils.first",
        "inputs": {
          "array": "{{shuffledNumbers}}",
          "count": 10
        },
        "outputAs": "firstTen"
      },
      {
        "id": "step4",
        "module": "utilities.array-utils.last",
        "inputs": {
          "array": "{{shuffledNumbers}}",
          "count": 10
        },
        "outputAs": "lastTen"
      },
      {
        "id": "step5",
        "module": "utilities.array-utils.sum",
        "inputs": {
          "array": "{{firstTen}}"
        },
        "outputAs": "sumFirstTen"
      },
      {
        "id": "step6",
        "module": "utilities.array-utils.average",
        "inputs": {
          "array": "{{lastTen}}"
        },
        "outputAs": "avgLastTen"
      },
      {
        "id": "step7",
        "module": "utilities.array-utils.min",
        "inputs": {
          "array": "{{baseNumbers}}"
        },
        "outputAs": "minNumber"
      },
      {
        "id": "step8",
        "module": "utilities.array-utils.max",
        "inputs": {
          "array": "{{baseNumbers}}"
        },
        "outputAs": "maxNumber"
      },
      {
        "id": "step9",
        "module": "utilities.array-utils.unique",
        "inputs": {
          "array": "{{shuffledNumbers}}"
        },
        "outputAs": "uniqueNumbers"
      },
      {
        "id": "step10",
        "module": "utilities.array-utils.chunk",
        "inputs": {
          "array": "{{baseNumbers}}",
          "size": 5
        },
        "outputAs": "chunkedArrays"
      },
      {
        "id": "step11",
        "module": "utilities.array-utils.flatten",
        "inputs": {
          "array": "{{chunkedArrays}}",
          "depth": 1
        },
        "outputAs": "flattenedNumbers"
      },
      {
        "id": "step12",
        "module": "utilities.array-utils.sortNumbers",
        "inputs": {
          "array": "{{shuffledNumbers}}",
          "order": "asc"
        },
        "outputAs": "sortedAsc"
      },
      {
        "id": "step13",
        "module": "utilities.array-utils.sortNumbers",
        "inputs": {
          "array": "{{shuffledNumbers}}",
          "order": "desc"
        },
        "outputAs": "sortedDesc"
      },
      {
        "id": "step14",
        "module": "utilities.math.add",
        "inputs": {
          "a": "{{sumFirstTen}}",
          "b": "{{avgLastTen}}"
        },
        "outputAs": "addResult"
      },
      {
        "id": "step15",
        "module": "utilities.math.multiply",
        "inputs": {
          "a": "{{minNumber}}",
          "b": 10
        },
        "outputAs": "multiplyResult"
      },
      {
        "id": "step16",
        "module": "utilities.math.divide",
        "inputs": {
          "a": "{{maxNumber}}",
          "b": 2
        },
        "outputAs": "divideResult"
      },
      {
        "id": "step17",
        "module": "utilities.math.round",
        "inputs": {
          "value": "{{avgLastTen}}",
          "decimals": 2
        },
        "outputAs": "roundedAvg"
      },
      {
        "id": "step18",
        "module": "utilities.math.floor",
        "inputs": {
          "value": "{{avgLastTen}}"
        },
        "outputAs": "floorResult"
      },
      {
        "id": "step19",
        "module": "utilities.math.ceil",
        "inputs": {
          "value": "{{avgLastTen}}"
        },
        "outputAs": "ceilResult"
      },
      {
        "id": "step20",
        "module": "utilities.math.abs",
        "inputs": {
          "value": -42
        },
        "outputAs": "absResult"
      },
      {
        "id": "step21",
        "module": "utilities.math.power",
        "inputs": {
          "base": 2,
          "exponent": 8
        },
        "outputAs": "powerResult"
      },
      {
        "id": "step22",
        "module": "utilities.math.sqrt",
        "inputs": {
          "value": 144
        },
        "outputAs": "sqrtResult"
      },
      {
        "id": "step23",
        "module": "utilities.math.modulo",
        "inputs": {
          "a": 17,
          "b": 5
        },
        "outputAs": "moduloResult"
      },
      {
        "id": "step24",
        "module": "utilities.math.percentage",
        "inputs": {
          "value": "{{sumFirstTen}}",
          "total": 200
        },
        "outputAs": "percentageResult"
      },
      {
        "id": "step25",
        "module": "utilities.math.clamp",
        "inputs": {
          "value": 150,
          "min": 0,
          "max": 100
        },
        "outputAs": "clampResult"
      },
      {
        "id": "step26",
        "module": "utilities.string-utils.toSlug",
        "inputs": {
          "text": "Hello World! This is a TEST",
          "options": {}
        },
        "outputAs": "slugResult"
      },
      {
        "id": "step27",
        "module": "utilities.string-utils.toCamelCase",
        "inputs": {
          "str": "hello world test"
        },
        "outputAs": "camelCase"
      },
      {
        "id": "step28",
        "module": "utilities.string-utils.toPascalCase",
        "inputs": {
          "str": "hello world test"
        },
        "outputAs": "pascalCase"
      },
      {
        "id": "step29",
        "module": "utilities.string-utils.toSnakeCase",
        "inputs": {
          "str": "helloWorldTest"
        },
        "outputAs": "snakeCase"
      },
      {
        "id": "step30",
        "module": "utilities.string-utils.toKebabCase",
        "inputs": {
          "str": "helloWorldTest"
        },
        "outputAs": "kebabCase"
      },
      {
        "id": "step31",
        "module": "utilities.string-utils.capitalize",
        "inputs": {
          "str": "hello world"
        },
        "outputAs": "capitalized"
      },
      {
        "id": "step32",
        "module": "utilities.string-utils.capitalizeWords",
        "inputs": {
          "str": "hello world from claude"
        },
        "outputAs": "capitalizedWords"
      },
      {
        "id": "step33",
        "module": "utilities.string-utils.reverse",
        "inputs": {
          "str": "automation"
        },
        "outputAs": "reversedString"
      },
      {
        "id": "step34",
        "module": "utilities.string-utils.truncate",
        "inputs": {
          "str": "This is a very long string that needs truncation",
          "maxLength": 20,
          "suffix": "..."
        },
        "outputAs": "truncatedString"
      },
      {
        "id": "step35",
        "module": "utilities.string-utils.stripHtml",
        "inputs": {
          "str": "<p>Hello <strong>World</strong></p>"
        },
        "outputAs": "strippedHtml"
      },
      {
        "id": "step36",
        "module": "utilities.string-utils.escapeHtml",
        "inputs": {
          "str": "<script>alert('test')</script>"
        },
        "outputAs": "escapedHtml"
      },
      {
        "id": "step37",
        "module": "utilities.encryption.hashSHA256",
        "inputs": {
          "data": "hello world"
        },
        "outputAs": "sha256Hash"
      },
      {
        "id": "step38",
        "module": "utilities.encryption.hashMD5",
        "inputs": {
          "data": "hello world"
        },
        "outputAs": "md5Hash"
      },
      {
        "id": "step39",
        "module": "utilities.encryption.encodeBase64",
        "inputs": {
          "data": "Hello World!"
        },
        "outputAs": "base64Encoded"
      },
      {
        "id": "step40",
        "module": "utilities.encryption.decodeBase64",
        "inputs": {
          "data": "{{base64Encoded}}"
        },
        "outputAs": "base64Decoded"
      },
      {
        "id": "step41",
        "module": "utilities.encryption.generateUUID",
        "inputs": {},
        "outputAs": "uuid1"
      },
      {
        "id": "step42",
        "module": "utilities.encryption.generateUUID",
        "inputs": {},
        "outputAs": "uuid2"
      },
      {
        "id": "step43",
        "module": "utilities.encryption.generateRandomString",
        "inputs": {
          "length": 16,
          "chars": "abcdefghijklmnopqrstuvwxyz0123456789"
        },
        "outputAs": "randomString"
      },
      {
        "id": "step44",
        "module": "utilities.datetime.now",
        "inputs": {},
        "outputAs": "currentDate"
      },
      {
        "id": "step45",
        "module": "utilities.datetime.formatDate",
        "inputs": {
          "date": "{{currentDate}}",
          "formatString": "yyyy-MM-dd"
        },
        "outputAs": "formattedDate"
      },
      {
        "id": "step46",
        "module": "utilities.datetime.addDays",
        "inputs": {
          "date": "{{currentDate}}",
          "days": 7
        },
        "outputAs": "futureDate"
      },
      {
        "id": "step47",
        "module": "utilities.datetime.subDays",
        "inputs": {
          "date": "{{currentDate}}",
          "days": 7
        },
        "outputAs": "pastDate"
      },
      {
        "id": "step48",
        "module": "utilities.datetime.addHours",
        "inputs": {
          "date": "{{currentDate}}",
          "hours": 24
        },
        "outputAs": "date24HoursLater"
      },
      {
        "id": "step49",
        "module": "utilities.datetime.toISO",
        "inputs": {
          "date": "{{currentDate}}"
        },
        "outputAs": "isoDate"
      },
      {
        "id": "step50",
        "module": "ai.ai-sdk.generateText",
        "inputs": {
          "options": {
            "prompt": "Generate a single creative product name for a tech gadget. Return ONLY the product name, nothing else.",
            "model": "gpt-4o-mini",
            "provider": "openai",
            "temperature": 0.9,
            "maxTokens": 20
          }
        },
        "outputAs": "aiProductName"
      },
      {
        "id": "step51",
        "module": "ai.ai-sdk.generateText",
        "inputs": {
          "options": {
            "prompt": "Generate a short tagline (max 10 words) for a workflow automation platform. Return ONLY the tagline.",
            "model": "gpt-4o-mini",
            "provider": "openai",
            "temperature": 0.8,
            "maxTokens": 30
          }
        },
        "outputAs": "aiTagline"
      },
      {
        "id": "step52",
        "module": "ai.ai-sdk.generateText",
        "inputs": {
          "options": {
            "prompt": "Generate a 2-sentence description of AI workflow automation. Be concise.",
            "model": "gpt-4o-mini",
            "provider": "openai",
            "temperature": 0.7,
            "maxTokens": 100
          }
        },
        "outputAs": "aiDescription"
      },
      {
        "id": "step53",
        "module": "utilities.array-utils.range",
